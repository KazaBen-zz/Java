package Vale;

class Test {
	  public static void main(String[] args) {
	    try {
	      System.out.println("Welcome to Java");
	      int i = 0;
	      int y = 2 / i;
	      System.out.println("Welcome to HTML");
	    }
	    catch(Exception e){
	    	System.out.println( " a");
	    }
	    finally {
	      System.out.println("The finally clause is executed");
	    }
	  }
	}