package Vale;
public class Test{
	  public static void main (String[] args){
	    SubClass s = new SubClass();
	    s.print();
	  }
	}

	class SuperClass{
	  int myID = 1;
	  SuperClass(){
		  myID=1;
	  }
	  public void print(){
	    System.out.println(myID);
	  }
	}
	
	class SubClass extends SuperClass{
	  int myID;
	  public SubClass(){
		  myID=2;
	  }
	}