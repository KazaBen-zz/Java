package AI.search2;
import sheffield.*;
public class RunPuzzleSearch {

	public static void main(String[] args) {
		EasyWriter screen = new EasyWriter();
		int[][] test1 = {{1,0,3},{4,2,6},{7,5,8}};
		int[][] target = {{1,2,3},{4,5,6},{7,8,0}};
		int[][] test2 = {{2,1,3},{4,5,6},{7,8,0}};

		PuzzleSearch searcher = new PuzzleSearch(test1, target);
		SearchState initState = (SearchState) new PuzzleState(test2);
		String resd = searcher.runSearch(initState, "depthFirst");
		screen.println(resd);
	}

}
