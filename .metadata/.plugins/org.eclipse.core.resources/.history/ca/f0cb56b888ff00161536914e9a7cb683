package Vale;
class Test {
	  public static void main(String[] args) {
	    SubClass s = new SubClass(1);
	  }
	}
	class SuperClass{
	  private int superClassField;
	    public SuperClass(int f){ 
	    superClassField = f + 1;
	    System.out.println("SuperClass constructor, field = " + superClassField);
	  }
	}
	class SubClass extends SuperClass{
	  public SubClass(int f){
	    System.out.println("SubClass constructor, field = " + f);
	  }
	}