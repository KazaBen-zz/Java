package Vale;

import java.util.*;

public class CountingSort{
 
 
  public static void main(String[] args) {
	  int k = 7;
	  int A[] = {3,1,0,2,1,1,2};
	  int C[] = new int[k+1];
	  for(int i=0; i<=k;i++){
		  C[i] = 0;
	  } 
	  for(int j=1;j<A.length;j++){
		  C[A[j]] = C[A[j]] + 1;
	  }
	  
	  System.out.println();
	  for(int i=0; i<k;i++){
		  System.out.print(A[i]+", ");
	  }/*
	  System.out.println();
	  for(int i=0; i<=k;i++){
		  System.out.print(C[i]+", ");
	  }
	  for(int i=1;i<=k;i++){
		  C[i] = C[i] + C[i-1];
	  }
	  System.out.println();
	  for(int i=0; i<=k;i++){
		  System.out.print(C[i]+", ");
	  }*/
  }
}
