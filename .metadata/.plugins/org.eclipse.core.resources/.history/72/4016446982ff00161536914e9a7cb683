package Vale;

		public class Testtt {
			  public static void main(String[] args) {
			    new Person().printPerson();
			    Student a = new Student();
			    a.printPerson();
			  }
			}


			class Person {
			  private String getInfo() {
			    return "Person";
			  }
				class Student extends Person {
					  private String getInfo() {
					    return "Student";
					  }
					}
  
			  public void printPerson() {
			    System.out.println(getInfo());
			  }
			}
	
